<?xml version="1.0" encoding="utf-8"?><androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="90dp"    android:layout_height="45dp"    android:layout_margin="5dp"    android:clickable="true"    android:elevation="10dp"    android:focusable="true"    android:foreground="?attr/selectableItemBackground"    app:cardCornerRadius="20dp">    <RelativeLayout        android:id="@+id/relativeRecyclerTypeEstateBckGroudn"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="@android:color/white">        <TextView            android:id="@+id/txtRecyclerTypeEstateName"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_centerInParent="true"            android:fontFamily="@font/roboto_regular"            android:text="Home"            android:textColor="@color/colorAccent" />    </RelativeLayout></androidx.cardview.widget.CardView>