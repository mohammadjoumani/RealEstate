<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="@color/background">    <androidx.viewpager2.widget.ViewPager2        android:id="@+id/viewPagerDialogSlideImage"        android:layout_width="match_parent"        android:layout_height="300dp"        android:layout_alignParentTop="true"        android:paddingStart="35dp"        android:paddingEnd="35dp" />    <RelativeLayout        android:id="@+id/rel1"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/viewPagerDialogSlideImage"        android:layout_marginTop="10dp">        <ImageView            android:id="@+id/imgIconAccount"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_marginStart="10dp"            android:layout_marginEnd="20dp"            android:src="@drawable/ic_account" />        <TextView            android:id="@+id/txtNameAccount"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_toEndOf="@+id/imgIconAccount"            android:fontFamily="@font/roboto_regular"            android:text="@string/office"            android:textColor="@color/colorPrimaryDark"            android:textSize="12dp" />        <TextView            android:id="@+id/txtOfficeNameDialogEstate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@+id/txtNameAccount"            android:layout_toEndOf="@+id/imgIconAccount"            android:fontFamily="@font/roboto_regular"            android:text="Office Name"            android:textColor="@color/colorAccent"            android:textSize="16dp" />        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layout_below="@id/txtOfficeNameDialogEstate"            android:layout_marginStart="30dp"            android:layout_marginTop="25dp"            android:layout_marginEnd="25dp"            android:background="@color/colorPrimaryDark" />    </RelativeLayout>    <RelativeLayout        android:id="@+id/rel2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/rel1"        android:layout_marginTop="10dp">        <ImageView            android:id="@+id/imageIconLocation"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_marginStart="10dp"            android:layout_marginEnd="20dp"            android:src="@drawable/ic_location" />        <TextView            android:id="@+id/txtNameLocation"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_toEndOf="@+id/imageIconLocation"            android:fontFamily="@font/roboto_regular"            android:text="@string/location"            android:textColor="@color/colorPrimaryDark"            android:textSize="12dp" />        <TextView            android:id="@+id/txtLocationDialogEstate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@+id/txtNameLocation"            android:layout_toEndOf="@+id/imageIconLocation"            android:fontFamily="@font/roboto_regular"            android:text="Location Name"            android:textColor="@color/colorAccent"            android:textSize="16dp" />        <View            android:layout_width="match_parent"            android:layout_height="2dp"            android:layout_below="@id/txtLocationDialogEstate"            android:layout_marginStart="30dp"            android:layout_marginTop="25dp"            android:layout_marginEnd="25dp"            android:background="@color/colorPrimaryDark" />    </RelativeLayout>    <RelativeLayout        android:id="@+id/rel3"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@+id/rel2"        android:layout_marginTop="10dp">        <ImageView            android:id="@+id/imageIconPrice"            android:layout_width="25dp"            android:layout_height="25dp"            android:layout_marginStart="10dp"            android:layout_marginEnd="20dp"            android:src="@drawable/ic_price" />        <TextView            android:id="@+id/txtPrice"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_toEndOf="@+id/imageIconPrice"            android:fontFamily="@font/roboto_regular"            android:text="@string/price"            android:textColor="@color/colorPrimaryDark"            android:textSize="12dp" />        <TextView            android:id="@+id/txtPriceDialogEstate"            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_below="@+id/txtPrice"            android:layout_toEndOf="@+id/imageIconPrice"            android:fontFamily="@font/roboto_regular"            android:text="100 $"            android:textColor="@color/colorAccent"            android:textSize="16dp" />    </RelativeLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:layout_below="@id/rel3"        android:layout_marginTop="25dp"        android:layout_marginBottom="25dp"        android:layout_marginStart="15dp"        android:layout_marginEnd="15dp">        <RelativeLayout            android:id="@+id/relCallPhone"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1">            <ImageView                android:id="@+id/imgCall"                android:layout_width="30dp"                android:layout_height="30dp"                android:src="@drawable/ic_call_green" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginStart="5dp"                android:layout_toEndOf="@+id/imgCall"                android:fontFamily="@font/roboto_regular"                android:text="Call Mobile"                android:textColor="@color/colorAccent"                android:textSize="16dp" />        </RelativeLayout>        <RelativeLayout            android:id="@+id/relWhatsapp"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1">            <ImageView                android:id="@+id/imgWhatsApp"                android:layout_width="30dp"                android:layout_height="30dp"                android:src="@drawable/ic_whatsapp" />            <TextView                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:layout_gravity="center"                android:layout_marginStart="5dp"                android:layout_toEndOf="@+id/imgWhatsApp"                android:fontFamily="@font/roboto_regular"                android:text="Whats App"                android:textColor="@color/colorAccent"                android:textSize="16dp" />        </RelativeLayout>    </LinearLayout></RelativeLayout>